include ../../Makefile.inc

all: client1 client2 client3 client4

client1:
	$(CXX) $(CXXFLAGS) $(LDFLAGS) client1.cpp -o client1
client2:
	$(CXX) $(CXXFLAGS) $(LDFLAGS) client2.cpp -o client2
client3:
	$(CXX) $(CXXFLAGS) $(LDFLAGS) client3.cpp -o client3
client4:
	$(CXX) $(CXXFLAGS) $(LDFLAGS) client4.cpp -o client4

clean:
	$(RM) client1 client2 client3 client4
